<h2>{{ blog.title }}</h2>

{% for article in blog.articles %}
    <div>
        <a href="{{ article.url }}">
            <h2>{{ article.title }}</h2>
            <p>{{ article.author }}</p>
            <p>{{ article.excerpt }}</p>
        </a>
    </div>
{% endfor %}

{%  schema  %} 
    {
        "name": "blog",
        "presets": [
            {
                "name": "blog"
            }
        ]
    }
{% endschema %}